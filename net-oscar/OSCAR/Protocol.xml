<?xml version="1.0"?>
<!DOCTYPE greeter SYSTEM "Protocol.dtd">

<oscar>
	<define name="flap">
		<byte>0x2A</byte> <!-- FLAP packet start indicator -->
		<byte name="channel">FLAP_CHAN_SNAC</byte>
		<word name="seqno" />
		<data length_prefix="word" name="msg" />
	</define>

	<define name="snac">
		<word name="family" />
		<word name="subtype" />
		<word name="flags" />
		<dword name="reqid" />
		<data name="data" />
	</define>

	<define name="userinfo">
		<data name="screenname" length_prefix="byte" />
		<word name="evil" />
		<tlvchain count_prefix="word">
			<tlv name="flags" type="1"><word /></tlv>
			<tlv name="membersince" type="2"><dword /></tlv>
			<tlv name="onsince" type="3"><dword /></tlv>
			<tlv name="idle" type="4"><word /></tlv>
			<tlv name="capabilities" type="13"><data length="16" count="-1" /></tlv>
			<tlv type="29">
				<tlvchain short="yes">
					<tlv name="icon_md5sum" type="1" shortno="1"><data /></tlv>
					<tlv name="extended_status" type="2"><data length_prefix="word" /></tlv>
				</tlvchain>
			</tlv>
		</tlvchain>
		<tlvchain>
			<tlv name="profile" type="2"><data /></tlv>
			<tlv name="awaymsg" type="4"><data /></tlv>
			<tlv name="chatdata" type="5"><data /></tlv>
		</tlvchain>
	</define>
</oscar>

